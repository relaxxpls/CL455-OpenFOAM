FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;

vertices
(
	(0 0 0)		// vertex 0


);

blocks
(
    hex (1 2 3 4 9 10 11 12) (10 10 500) simpleGrading (1 1 1)	// block 1
    
);


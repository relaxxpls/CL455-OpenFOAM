(
    arc 5 6 (0 2 0)		// edge 1
);

boundary
(
    inlet
    {
        type patch;
        faces
        (
            (1 2 3 4)
         );
    }
    outlet
    {
        type patch;
        faces
        (
            (9 10 11 12)
         );
    }
    wall
    {
        type wall;
        faces
        (
            (13 14 6 5)
        );
    }
);

mergePatchPairs
(
